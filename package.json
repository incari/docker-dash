{
  "name": "docker-dashboard",
  "version": "1.0.0",
  "description": "Docker Dashboard for managing container shortcuts",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev:backend": "node --watch src/server.js",
    "dev:frontend": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
  "dependencies": {
    "better-sqlite3": "^9.4.3",
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "express": "^4.18.2",
    "multer": "^2.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
